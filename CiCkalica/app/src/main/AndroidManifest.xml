<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="studnetsproject.air.foi.cickalica">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".game_result"
            android:label="@string/title_activity_game_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".cc_modul"
            android:label="@string/title_activity_cc_modul"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".score"
            android:label="@string/title_activity_score"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ijeje_modul"
            android:label="@string/title_activity_ijeje_modul"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
